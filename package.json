    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "browser-sync": "^2.12.8",
    "chokidar-cli": "^1.2.0",
    "corser": "^2.0.1",
    "css-loader": "^0.23.1",
    "eslint": "^2.10.2",
    "eslint-config-airbnb": "^9.0.1",
