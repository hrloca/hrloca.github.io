{
  "name": "github.hrloca.io",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "npm run pre",
    "pre": "npm run clean && npm run build",
    "dev": "npm run watch & npm run bs",
    "clean": "rm -rf www & rm -rf dist",
    "build": "webpack --progress --colors",
    "watch": "webpack -d --progress --colors --watch",
    "bs": "browser-sync start -s './' --ss ./dist --files='./*.html, ./dist/css/**/*.css, ./dist/js/**/*.js, ./dist/assets/svg/sprite.svg'",
    "test": "mocha --compilers js:espower-babel/guess"
  },
