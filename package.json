{
  "name": "min",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "npm run pre",
    "dev": "npm run watch & npm run html:dev & npm run cp:manifest & npm run bsync",
    "prod": "npm run clean && npm run build && npm run cp:manifest && npm run html:build",
    "build": "webpack --progress --colors",
    "watch": "webpack -d --progress --colors --watch",
    "html:dev": "node metalsmith.js",
    "html:build": "node metalsmith.js",
    "cp:manifest": "cp -rf ./src/manifest/ ./docs/manifest/",
    "clean": "rm -rf docs",
    "bsync": "browser-sync start -s './docs' --files='./docs/*.html, ./docs/css/**/*.css, ./docs/js/**/*.js'"
  },
  "devDependencies": {
    "ava": "^0.18.0",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "browser-sync": "^2.12.8",
    "css-loader": "^0.26.1",
    "eslint": "^3.14.1",
    "eslint-loader": "^1.6.1",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.10.0",
    "handlebars": "^4.0.6",
    "metalsmith": "^2.1.0",
    "metalsmith-html-minifier": "^2.2.0",
    "metalsmith-ignore": "^0.1.2",
    "metalsmith-in-place": "^1.4.4",
    "metalsmith-inline-source": "^1.0.4",
    "metalsmith-layouts": "^1.6.5",
    "metalsmith-partial": "^0.1.0",
    "metalsmith-watch": "^1.0.3",
    "postcss": "^5.2.11",
    "postcss-cssnext": "^2.9.0",
    "postcss-extend": "^1.0.5",
    "postcss-import": "^9.1.0",
    "postcss-loader": "^1.2.2",
    "postcss-reporter": "^3.0.0",
    "qs": "^6.3.0",
    "raw-loader": "^0.5.1",
    "resolve-url-loader": "^1.6.1",
    "style-loader": "^0.13.1",
    "stylelint": "^7.7.1",
    "svg-url-loader": "^2.0.1",
    "svgo": "^0.7.2",
    "svgo-loader": "^1.1.2",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1",
    "webpack-build-notifier": "^0.1.13",
    "webpack-dev-server": "^2.3.0"
  },
  "dependencies": {
    "history": "^4.5.1",
    "immutable": "^3.8.1",
    "isomorphic-fetch": "^2.2.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "react-router-dom": "^4.0.0-beta.6",
    "redux": "^3.6.0"
  }
}
